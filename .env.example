# Example environment variables for Mini-CRM
# Copy this file to .env and adjust per environment (development/test/production)

# Application
NODE_ENV=development
APP_PORT=3000

# Database (Postgres)
DB_HOST=127.0.0.1
DB_PORT=5432
DB_NAME=mini_crm
DB_USER=postgres
DB_PASS=postgres

# Secrets
JWT_SECRET=change_this_to_a_secure_value

# Logging
LOG_LEVEL=debug
